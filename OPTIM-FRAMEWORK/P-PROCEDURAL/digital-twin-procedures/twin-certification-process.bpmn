<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="4.12.0">
  <bpmn:process id="TwinCertificationProcess" name="Twin Certification Process" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1" name="Certification Request">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:task id="Task_PrepareEvidence" name="Prepare Evidence Package">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_TechnicalReview" name="Technical Review">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_ValidationTests" name="Execute Validation Tests">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_BoardApproval" name="Board Approval">
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:task>
    <bpmn:endEvent id="EndEvent_1" name="Twin Certified">
      <bpmn:incoming>Flow_5</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="Task_PrepareEvidence" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_PrepareEvidence" targetRef="Task_TechnicalReview" />
    <bpmn:sequenceFlow id="Flow_3" sourceRef="Task_TechnicalReview" targetRef="Task_ValidationTests" />
    <bpmn:sequenceFlow id="Flow_4" sourceRef="Task_ValidationTests" targetRef="Task_BoardApproval" />
    <bpmn:sequenceFlow id="Flow_5" sourceRef="Task_BoardApproval" targetRef="EndEvent_1" />
  </bpmn:process>
</bpmn:definitions>