<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" id="Definitions_1" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="4.12.0">
  <bpmn:process id="DataQualityWorkflow" name="Data Quality Workflow" isExecutable="true">
    <bpmn:startEvent id="StartEvent_1" name="Data Ingestion">
      <bpmn:outgoing>Flow_1</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:task id="Task_ValidateSchema" name="Validate Schema">
      <bpmn:incoming>Flow_1</bpmn:incoming>
      <bpmn:outgoing>Flow_2</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_CheckQuality" name="Quality Checks">
      <bpmn:incoming>Flow_2</bpmn:incoming>
      <bpmn:outgoing>Flow_3</bpmn:outgoing>
    </bpmn:task>
    <bpmn:exclusiveGateway id="Gateway_QualityPass" name="Quality Pass?">
      <bpmn:incoming>Flow_3</bpmn:incoming>
      <bpmn:outgoing>Flow_4</bpmn:outgoing>
      <bpmn:outgoing>Flow_5</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:task id="Task_CleanseData" name="Cleanse Data">
      <bpmn:incoming>Flow_5</bpmn:incoming>
      <bpmn:outgoing>Flow_6</bpmn:outgoing>
    </bpmn:task>
    <bpmn:task id="Task_PublishData" name="Publish Clean Data">
      <bpmn:incoming>Flow_4</bpmn:incoming>
      <bpmn:incoming>Flow_6</bpmn:incoming>
      <bpmn:outgoing>Flow_7</bpmn:outgoing>
    </bpmn:task>
    <bpmn:endEvent id="EndEvent_1" name="Data Published">
      <bpmn:incoming>Flow_7</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_1" sourceRef="StartEvent_1" targetRef="Task_ValidateSchema" />
    <bpmn:sequenceFlow id="Flow_2" sourceRef="Task_ValidateSchema" targetRef="Task_CheckQuality" />
    <bpmn:sequenceFlow id="Flow_3" sourceRef="Task_CheckQuality" targetRef="Gateway_QualityPass" />
    <bpmn:sequenceFlow id="Flow_4" name="Pass" sourceRef="Gateway_QualityPass" targetRef="Task_PublishData" />
    <bpmn:sequenceFlow id="Flow_5" name="Fail" sourceRef="Gateway_QualityPass" targetRef="Task_CleanseData" />
    <bpmn:sequenceFlow id="Flow_6" sourceRef="Task_CleanseData" targetRef="Task_PublishData" />
    <bpmn:sequenceFlow id="Flow_7" sourceRef="Task_PublishData" targetRef="EndEvent_1" />
  </bpmn:process>
</bpmn:definitions>